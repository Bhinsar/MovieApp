{
  "expo": {
    "name": "Movie App",
    "slug": "MovieApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#030014",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#030014"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#030014"
      },
      "package": "com.anonymous.MovieApp"
    },

    "web": {
      "bundler": "metro",
      "output": "static"
    }
  }
}